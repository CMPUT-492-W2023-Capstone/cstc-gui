{
  "compilerOptions": {
    "noEmit": true,
    "module": "ESNext",
    "target": "ESNext",
    "types": ["node"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
    "paths": {
        "*": [
            "node_modules/*",
            "src/types/*",
        ]
    }
  }
}
